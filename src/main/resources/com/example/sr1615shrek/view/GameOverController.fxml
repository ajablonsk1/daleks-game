<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sr1615shrek.view.GameOverController">
    <VBox layoutY="430.0" prefHeight="141.0" prefWidth="1157.0" spacing="20" style="-fx-text-alignment: center; -fx-alignment: center" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <Text fx:id="text">
            <font>
               <Font name="System Bold Italic" size="40.0" />
            </font></Text>
            <Button fx:id="button" onAction="#playAgain" prefHeight="24.0" prefWidth="120.0" text="PLAY AGAIN" wrapText="true" />
            <Button fx:id="home" onAction="#goToHomePage" prefHeight="24.0" prefWidth="120.0" text="HOME PAGE" wrapText="true" />
        </children>
    </VBox>
</AnchorPane>
